generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Operacao {
  id        Int @id @default(autoincrement())
  tipo      Int //'1- ADICAO' | '2- SUBTRACAO' | '3- MULTIPLICACAO' | '4- DIVISAO'
  valor1    Float 
  valor2    Float
  resultado Float

  @@map("operacoes")
}